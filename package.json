{
  "name": "obsidian-file-navigator",
  "version": "1.0.1",
  "description": "Navigate between filtered and sorted files around the current note in Obsidian.",
  "main": "main.js",
  "types": "types.d.ts",
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "build:brat": "npm run build && node ./scripts/sync-brat-assets.mjs",
    "test": "node ./scripts/run-tests.mjs",
    "test:watch": "node ./scripts/run-tests.mjs --watch"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "navigation"
  ],
  "author": "",
  "license": "MIT",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^18.19.21",
    "@vitest/coverage-v8": "^1.4.0",
    "jsdom": "^27.0.0",
    "obsidian": "^1.4.11",
    "rollup": "^3.29.4",
    "rollup-plugin-copy": "^3.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  }
}
